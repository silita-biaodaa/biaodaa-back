<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.silita.dao.TbRoleModuleMapper" >


  <insert id="insertRoleModule" parameterType="map">
    insert into biaodaa_admin.`tb_role_module`(rid,id)
    values
    <foreach collection="list" item="list" separator=",">
      (#{list.rid},#{list.id})
    </foreach>
  </insert>

  <delete id="deleteRoleModule" parameterType="map">
    DELETE FROM biaodaa_admin.tb_role_module where rid = #{rid}
  </delete>

</mapper>