<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.silita.dao2.AllZhMapper" >
  <resultMap id="BaseResultMap" type="com.silita.model.AllZh" >
    <!--
      WARNING - @mbg.generated
    -->
    <result column="uuid" property="uuid" jdbcType="VARCHAR" />
    <result column="type" property="type" jdbcType="VARCHAR" />
    <result column="mainUuid" property="mainUuid" jdbcType="VARCHAR" />
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="rank" property="rank" jdbcType="VARCHAR" />
    <result column="finalUuid" property="finalUuid" jdbcType="VARCHAR" />
  </resultMap>

  <select id="listAllZhByName" parameterType="com.silita.model.AllZh" resultMap="BaseResultMap">
    SELECT *
    FROM mishu.all_zh
    WHERE `rank` IS NOT NULL AND `finalUUid` IS NOT NULL AND `name` LIKE CONCAT('%', #{name}, '%')
    LIMIT 0,10
  </select>
